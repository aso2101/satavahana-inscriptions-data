	<ab>
	  <lb n="1"/>
	  raño 
	  <persName key="pers:ŚrīSātakarṇi">
	    siri sātakaṃṇi<lb n="2" break="no"/>sa
	  </persName>
	  <date when-custom="30" datingMethod="cal:regnal" datingPoint="pers:ŚrīSātakarṇi">tise saṃvachare</date>

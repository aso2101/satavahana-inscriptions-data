      <div type="edition" xml:lang="pra-Latn">
	<div type="textpart" subtype="section" n="93">
	  <head xml:lang="eng">93</head>
	  <p>
	    <supplied reason="lost">bo</supplied>dhisato somarāto
	  </p>
	</div>
	<div type="textpart" subtype="section" n="94">
	  <head xml:lang="eng">94</head>
	  <p>
	    rāyo <persName key="pers:Mantalaka">mātalako</persName>
	  </p>
	</div>
	<div type="textpart" subtype="section" n="95">
	  <head xml:lang="eng">95</head>
	  <p>
	    rāyā <persName key="pers:Aśoka">asoko</persName>
	  </p>
	</div>
	<div type="textpart" subtype="section" n="96-A">
	  <head xml:lang="eng">96-A</head>
	  <p>
	    rājā <persName key="pers:VpŚrīChimuka">siri chimuka sādavāhano</persName>
	  </p>
	</div>
	<div type="textpart" subtype="section" n="96-B">
	  <head xml:lang="eng">96-B</head>
	  <p>
	    nāgarāyasa <unclear>sakhadhāvi</unclear>
	  </p>
	</div>
	<div type="textpart" subtype="section" n="97">
	  <head xml:lang="eng">97</head>
	  <p>
	    rāyā <persName key="pers:Aśoka">asoko</persName>
	  </p>
	</div>
	<div type="textpart" subtype="section" n="98">
	  <head xml:lang="eng">98</head>
	  <p>
	    rāya cakavaṭi satarajano
	  </p>
	</div>
	<div type="textpart" subtype="section" n="99">
	  <head xml:lang="eng">99</head>
	  <p>
	    rāya <persName key="pers:VpŚrīPuḷumāvi">pudumāvi</persName> ajayatasa ujeni deti
	  </p>
	</div>
	<div type="textpart" subtype="section" n="100">
	  <head xml:lang="eng">100</head>
	  <p>
	    rāya <supplied reason="lost">u</supplied>dayano
	  </p>
	</div>
	<div type="textpart" subtype="section" n="102">
	  <head xml:lang="eng">102</head>
	  <p>
	    rāya <persName key="pers:ŚrīSātakarṇi">sātakaṃni<supplied reason="lost">sa</supplied></persName>
	    gayasara himayāni payumāni deṇo yeti
	  </p>
	</div>

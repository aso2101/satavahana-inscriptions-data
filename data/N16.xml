	<p>
	  <lb n="1"/>
	  sidhaṃ <persName key="pers:Śivamitra">sivamita</persName>lekhakaputasa 
	  <lb n="2"/>
	  <persName key="pers:Rāmaṇaka-2">rāmaṃṇakasa</persName> leṇam
	  deya<app
	    ><lem source="#N">dhaṃmaṃ</lem>
	     <rdg source="#G">dhamaṃ</rdg>
	  </app>
	</p> 

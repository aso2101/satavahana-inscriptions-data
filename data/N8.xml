	  dāsakasa
	  <persName key="pers:Mugūdāsa">mu<app
	    ><lem>gū</lem><rdg wit="#AS">gu</rdg></app>dāsasa</persName>
	  saparivārasa le<app><lem>ṇa</lem><rdg wit="#G">ṇaṃ</rdg></app>
